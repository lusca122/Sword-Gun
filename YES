    		c.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0, 0, 0)
    		if c ~= SCYTHE.PrimaryPart then
    			weldBetween(SCYTHE.PrimaryPart,c)
    		end
    		c.Locked = true
    	end
    end
    local FIRE = SCYTHE.Neon.Fire
