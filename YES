--Varibles
local permaDeath = false
local bot = false
local pressingShift = false
local reanimated = false
local botHats = {}

--Resetting GUI Value
if not game.CoreGui:FindFirstChild("ResetGUIValue") then
    local resetGUIValue = Instance.new("BoolValue", game.CoreGui)
    resetGUIValue.Name = "ResetGUIValue"
end

local function BlindReaper()
    --Variables
    local player = game:GetService("Players").LocalPlayer
    local character = player.Character
    local hrp = character.HumanoidRootPart
    local reanimation = character:WaitForChild("FakeCharacter").Reanimation
    
    reanimated = true
    hrp.Transparency = 0
    
    local flinging = false
    local getChild = nil
    
    local function GetCHILD(CHILD)
        if CHILD:FindFirstChildWhichIsA("Humanoid").RigType == Enum.HumanoidRigType.R6 then
            getChild = CHILD.Torso
        else
            getChild = CHILD.UpperTorso
        end
        print(getChild)
    end
    
    local bodyAngularVelocity = Instance.new("BodyAngularVelocity", hrp)
    bodyAngularVelocity.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
    bodyAngularVelocity.P = 1000000000000000000000000000
    bodyAngularVelocity.AngularVelocity = Vector3.new(10000, 10000, 10000)
    
    local function Positioning()
        --Positioning BodyParts
        for i,v in pairs(character:GetChildren()) do
            if v:IsA("Part") and v.Name ~= "HumanoidRootPart" and character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
                v.CFrame = reanimation:FindFirstChild(v.Name).CFrame
            end
        end
        
        if flinging == true then
            hrp.Position = getChild.Position
            
            if character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
                if bodyAngularVelocity.AngularVelocity == Vector3.new(0, 0, 0) then
                    bodyAngularVelocity.AngularVelocity = Vector3.new(10000, 10000, 10000)
                end
            end
        else
            if character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
                hrp.Position = Vector3.new(reanimation.Torso.Position.X, -50, reanimation.Torso.Position.Z)
            else
                hrp.Position = reanimation.Torso.Position
                if bodyAngularVelocity.AngularVelocity == Vector3.new(10000, 10000, 10000) then
                    bodyAngularVelocity.AngularVelocity = Vector3.new(0, 0, 0)
                end
            end
        end
        
        --R15
        if character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
            --Head
            character.Head.CFrame = reanimation.Head.CFrame
            
            --Torso
            character.UpperTorso.CFrame = reanimation.Torso.CFrame * CFrame.new(0, 0.185, 0)
            character.LowerTorso.CFrame = reanimation.Torso.CFrame * CFrame.new(0, -0.8, 0)
            
            --HumanoidRootPart
            --character.HumanoidRootPart.CFrame = cHRP.CFrame
            
            --Left Arm
            character.LeftUpperArm.CFrame = reanimation["Left Arm"].CFrame * CFrame.new(0, 0.4, 0)
            character.LeftLowerArm.CFrame = reanimation["Left Arm"].CFrame * CFrame.new(0, -0.19, 0)
            character.LeftHand.CFrame = reanimation["Left Arm"].CFrame * CFrame.new(0, -0.84, 0)
            
            --Right Arm
            character.RightUpperArm.CFrame = reanimation["Right Arm"].CFrame * CFrame.new(0, 0.4, 0)
            character.RightLowerArm.CFrame = reanimation["Right Arm"].CFrame * CFrame.new(0, -0.19, 0)
            character.RightHand.CFrame = reanimation["Right Arm"].CFrame * CFrame.new(0, -0.84, 0)
            
            --Left Leg
            character.LeftUpperLeg.CFrame = reanimation["Left Leg"].CFrame * CFrame.new(0, 0.55, 0)
            character.LeftLowerLeg.CFrame = reanimation["Left Leg"].CFrame * CFrame.new(0, -0.19, 0)
            character.LeftFoot.CFrame = reanimation["Left Leg"].CFrame * CFrame.new(0, -0.85, 0)
            
            --Right Leg
            character.RightUpperLeg.CFrame = reanimation["Right Leg"].CFrame * CFrame.new(0, 0.55, 0)
            character.RightLowerLeg.CFrame = reanimation["Right Leg"].CFrame * CFrame.new(0, -0.19, 0)
            character.RightFoot.CFrame = reanimation["Right Leg"].CFrame * CFrame.new(0, -0.85, 0)
        end
        
        --Positioning The Hats
        for i,v in pairs(character:GetChildren()) do
            if v:IsA("Accessory") and v.Name ~= "Dark Matter Sword" and v.Name ~= "Red Sci-Fi Scattergun" then
                if bot == false then
                    v.Handle.CFrame = reanimation:FindFirstChild(v.Name).Handle.CFrame
                else
                    --Arms and Legs
                    if v.Name ~= "Head" and reanimation:FindFirstChild(v.Name) then
                        v.Handle.CFrame = reanimation:FindFirstChild(v.Name).CFrame * CFrame.Angles(1.5708, 0, 0)
                    end
                    
                    --Head
                    if v.Name == "Head" then
                        --Medi Hood
                        if v.Handle:FindFirstChildWhichIsA("SpecialMesh").MeshId == "rbxassetid://617474228" then
                            v.Handle.CFrame = reanimation:FindFirstChild(v.Name).CFrame * CFrame.new(0, -0.025, 0.23)
                        end
                        
                        --Shadowed Head
                        if v.Handle:FindFirstChildWhichIsA("SpecialMesh").MeshId == "rbxassetid://4904532191" then
                            v.Handle.CFrame = reanimation:FindFirstChild(v.Name).CFrame
                        end
                    end
                    
                    --Left Half Of The Torso
                    if v.Name == "Torso1" then
                        v.Handle.CFrame = reanimation.Torso.CFrame * CFrame.new(-0.5, 0, 0) * CFrame.Angles(1.5708, 0, 0)
                    end
                    
                    --Right Half Of The Torso
                    if v.Name == "Torso2" then
                        v.Handle.CFrame = reanimation.Torso.CFrame * CFrame.new(0.5, 0, 0) * CFrame.Angles(1.5708, 0, 0)
                    end
                end
            end
            
            if v.Name == "Dark Matter Sword" then
                v.Handle.CFrame = reanimation:WaitForChild("Scythe"):WaitForChild("Base").CFrame * CFrame.new(0, -0.746, -2.9000001) * CFrame.Angles(11.63, 3.15, 0)
            end
            
            if v.Name == "Black Type-37 Pulse Rifle" then
                v.Handle.CFrame = reanimation:WaitForChild("Shotgun"):WaitForChild("Base").CFrame * CFrame.new(0, -0.4, -0.15) * CFrame.Angles(-0.61, -1.57, 0)
            end
        end
        game:GetService("RunService").Heartbeat:wait()
    end
    
    --Scythe--
    --Scythe
    local scythe = Instance.new("Model", workspace)
    scythe.Name = "Scythe"
    
    --Blade
    local blade = Instance.new("Part", scythe)
    blade.BottomSurface = Enum.SurfaceType.Smooth
    blade.CanCollide = false
    blade.CFrame = CFrame.new(0,-1.7379796504974,-4.2933163642883,4.3047307940469e-08,-3.6120997748412e-08,1,0.98480778932571,0.17364805936813,-3.6121001301126e-08,-0.17364805936813,0.98480778932571,4.3047311493183e-08)
    blade.Rotation = Vector3.new(80,90,0)
    blade.Name = "Blade"
    blade.Position = Vector3.new(0,-1.7379796504974,-4.2933163642883)
    blade.Orientation = Vector3.new(0,90,80)
    blade.Size = Vector3.new(5.6570000648499,2.1480000019073,0.37900000810623)
    blade.Anchored = true
    blade.TopSurface = Enum.SurfaceType.Smooth
    
    --Attachment1
    local attachment1 = Instance.new("Attachment", blade)
    attachment1.CFrame = CFrame.new(2.4502875804901,-0.22234107553959,6.8230664174163e-13,1,0,0,0,1,0,0,0,1)
    attachment1.Name = "Attachment1"
    attachment1.Position = Vector3.new(2.4502875804901,-0.22234107553959,6.8230664174163e-13)
    
    --Attachment0
    local attachment0 = Instance.new("Attachment", blade)
    attachment0.CFrame = CFrame.new(2.2131624221802,0.81533551216125,6.7040638448632e-13,1,0,0,0,1,0,0,0,1)
    attachment0.Name = "Attachment0"
    attachment0.Position = Vector3.new(2.2131624221802,0.81533551216125,6.7040638448632e-13)
    
    --Handle
    local scytheHandle = Instance.new("Part", scythe)
    scytheHandle.BottomSurface = Enum.SurfaceType.Smooth
    scytheHandle.CanCollide = false
    scytheHandle.CFrame = CFrame.new(0,7.2141972395912e-09,-2.0740005970001,-1,-4.371137052317e-08,-4.3711381181311e-08,4.3711398944879e-08,4.4703483581543e-08,-1,4.3711391839452e-08,-1,-4.4703483581543e-08)
    scytheHandle.Rotation = Vector3.new(90,0,180)
    scytheHandle.Name = "Handle"
    scytheHandle.Position = Vector3.new(0,7.2141972395912e-09,-2.0740005970001)
    scytheHandle.Orientation = Vector3.new(90,-180,0)
    scytheHandle.Size = Vector3.new(0.20000000298023,6.9250001907349,0.20000000298023)
    scytheHandle.Anchored = true
    scytheHandle.TopSurface = Enum.SurfaceType.Smooth
    
    --Head
    local scytheHead = Instance.new("Part", scythe)
    scytheHead.BottomSurface = Enum.SurfaceType.Smooth
    scytheHead.CanCollide = false
    scytheHead.CFrame = CFrame.new(0,0.34829646348953,-4.2573823928833,-4.3711388286738e-08,-1,-1.0658141036402e-14,1,-4.3711381181311e-08,0,0,5.3290705182008e-15,1)
    scytheHead.Rotation = Vector3.new(0,0,90)
    scytheHead.Name = "Head"
    scytheHead.Position = Vector3.new(0,0.34829646348953,-4.2573823928833)
    scytheHead.Orientation = Vector3.new(0,0,90)
    scytheHead.Size = Vector3.new(1.0099999904633,0.20999999344349,1.0800000429153)
    scytheHead.Anchored = true
    scytheHead.TopSurface = Enum.SurfaceType.Smooth
    
    --Neon
    local neon = Instance.new("Part", scythe)
    neon.BottomSurface = Enum.SurfaceType.Smooth
    neon.CanCollide = false
    neon.CFrame = CFrame.new(0,-1.8212832212448,-4.6391997337341,-4.3047307940469e-08,-5.1301785930491e-08,1,-0.98480778932571,-0.1736481487751,-5.1301803694059e-08,0.1736481487751,-0.98480778932571,-4.3047325704038e-08)
    neon.Rotation = Vector3.new(-100,90,0)
    neon.Name = "Neon"
    neon.Position = Vector3.new(0,-1.8212832212448,-4.6391997337341)
    neon.Orientation = Vector3.new(0,90,-100)
    neon.Size = Vector3.new(5.058000087738,0.93099999427795,0.37000000476837)
    neon.Anchored = true
    neon.TopSurface = Enum.SurfaceType.Smooth
    
    --Particle Emitter
    local fire = Instance.new("ParticleEmitter", neon)
    fire.Enabled = false
    fire.Acceleration = Vector3.new(0,25,0)
    fire.VelocitySpread = 360
    fire.Texture = "rbxasset://textures/particles/fire_main.dds"
    fire.Color = ColorSequence.new({ColorSequenceKeypoint.new(0,Color3.new(1,0,0)),ColorSequenceKeypoint.new(0.50083196163177,Color3.new(88/255,2/17,5/51)),ColorSequenceKeypoint.new(1,Color3.new(88/255,2/17,5/51))})
    fire.LightEmission = 1
    fire.ZOffset = 0.30000001192093
    fire.Rotation = NumberRange.new(0,360)
    fire.LightInfluence = 1
    fire.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0,1,0),NumberSequenceKeypoint.new(0.043203368782997,0.62087911367416,0),NumberSequenceKeypoint.new(0.20021073520184,0.27472525835037,0),NumberSequenceKeypoint.new(1,0,0)})
    fire.Name = "Fire"
    fire.Lifetime = NumberRange.new(0.20000000298023,0.69999998807907)
    fire.Speed = NumberRange.new(1,4)
    fire.SpreadAngle = Vector2.new(360,360)
    fire.Rate = 555
    fire.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,1,0),NumberSequenceKeypoint.new(0.59470897912979,0.97826063632965,0),NumberSequenceKeypoint.new(0.81587302684784,0.6521737575531,0),NumberSequenceKeypoint.new(1,0,0)})
    
    --Base
    local scytheBase = Instance.new("Part", scythe)
    scytheBase.Size = Vector3.new(0.050000000745058,0.050000000745058,0.050000000745058)
    scytheBase.Name = "Base"
    scytheBase.CanCollide = false
    scytheBase.BottomSurface = Enum.SurfaceType.Smooth
    scytheBase.CFrame = CFrame.new(0,0,0,1,-1.1102230246252e-14,-1.0658141036402e-14,-1.1102230246252e-14,1,0,-1.0658141036402e-14,0,1)
    scytheBase.Anchored = true
    scytheBase.TopSurface = Enum.SurfaceType.Smooth
    
    --Trail
    local scytheTrail = Instance.new("Trail", scythe)
    scytheTrail.Enabled = false
    scytheTrail.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0,0.22499996423721,0),NumberSequenceKeypoint.new(0.4913891851902,0.77499997615814,0),NumberSequenceKeypoint.new(0.7485648393631,0.93124997615814,0),NumberSequenceKeypoint.new(1,1,0)})
    scytheTrail.Lifetime = 0.20000000298023
    scytheTrail.Color = ColorSequence.new({ColorSequenceKeypoint.new(0,Color3.new(88/255,2/17,5/51)),ColorSequenceKeypoint.new(0.29450914263725,Color3.new(88/255,2/17,5/51)),ColorSequenceKeypoint.new(1,Color3.new(1,1,1))})
    scytheTrail.Attachment0 = attachment0
    scytheTrail.LightInfluence = 1
    scytheTrail.Attachment1 = attachment1
    
    scythe.PrimaryPart = scytheBase
    
    for i,v in pairs(scythe:GetChildren()) do
        if v:IsA("Part") then
            v.Transparency = 1
        end
    end
    
    --Shotgun--
    --Shotgun
    local shotgun = Instance.new("Model", workspace)
    shotgun.Name = "Shotgun"
    
    --Barrels
    local barrels = Instance.new("Part", shotgun)
    barrels.CanCollide = false
    barrels.Rotation = Vector3.new(90,0,90)
    barrels.Size = Vector3.new(0.25,2.75,0.6370000243187)
    barrels.Name = "Barrels"
    barrels.Position = Vector3.new(-1.5750122070313,0.48600000143051,2.3500061035156)
    barrels.Orientation = Vector3.new(90,-90,0)
